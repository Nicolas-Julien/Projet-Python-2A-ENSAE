   "source": [
    "df = df.rename(columns = {'FTR': 'Result'})\n",
    "\n",
    "def Resultat(r):\n",
    "  if r == 'H':\n",
    "    return(3)\n",
    "  elif r=='D':\n",
    "    return(1)\n",
    "  else :\n",
    "    return(0)\n",
    "\n",
    "df['Result'] = df['Result'].apply(Resultat)\n",
    "display(df)"
   ]
